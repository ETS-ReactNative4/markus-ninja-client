  "scripts": {
    "build:css": "node-sass-chokidar --include-path ./node_modules ./src -o ./src",
    "watch:css": "run-s build:css \"build:css --watch --recursive\"",
    "build:relay": "node ./setup",
    "prestart": "npm run build:relay",
    "start:js": "react-scripts start",
    "start": "run-p watch:css start:js",
    "prebuild": "npm run build:relay",
    "build": "npm run build:css && react-scripts build",
    "pretest": "npm run build:relay",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "relay": "curl http://localhost:5000/graphql/schema -o ./src/graphql.json && relay-compiler --src ./src --schema ./src/graphql.json"
