{
  "name": "clientv2",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "base64url": "^3.0.0",
    "classnames": "^2.2.6",
    "draft-js": "^0.10.5",
    "graphql": "^0.13.2",
    "htmr": "^0.7.0",
    "moment": "^2.22.2",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "pluralize": "^7.0.0",
    "react": "^16.4.2",
    "react-color": "^2.14.1",
    "react-dom": "^16.4.2",
    "react-md": "^1.4.2",
    "react-relay": "^1.6.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "relay-runtime": "^1.6.2",
    "webfontloader": "^1.6.28"
  },
  "scripts": {
    "build:css": "node-sass-chokidar --include-path ./node_modules ./src/styles -o ./src/styles",
    "watch:css": "run-s build:css \"build:css --watch --recursive\"",
    "build:relay": "node ./setup",
    "prestart": "npm run build:relay",
    "start:js": "react-scripts start",
    "start": "run-p watch:css start:js",
    "prebuild": "npm run build:relay",
    "build": "npm run build:css && react-scripts build",
    "pretest": "npm run build:relay",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "relay": "curl http://localhost:5000/graphql/schema -o ./src/graphql.json && relay-compiler --src ./src --schema ./src/graphql.json"
  },
  "devDependencies": {
    "babel-plugin-relay": "^1.6.2",
    "relay-compiler": "^1.6.2"
  }
}
